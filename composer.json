{
  "name": "gertvdb/settings",
  "description": "Collection for site settings",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "minimum-stability": "alpha",
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "drupal/coder": "^8.3",
    "drupal/core-recommended": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\settings\\": "src/",
      "Drupal\\settings_branding\\": "modules/settings_branding/src/",
      "Drupal\\settings_communication\\": "modules/settings_communication/src/",
      "Drupal\\settings_social\\": "modules/settings_social/src/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
