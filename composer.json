{
  "name": "gertvdb/settings",
  "description": "Collection for site settings",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "minimum-stability": "alpha",
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "drupal/hook_event_dispatcher": "^1.19"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "extra": {
    "installer-paths": {
      "vendor/drupal/{$name}": ["drupal/core"],
      "vendor/drupal/hook_event_dispatcher": ["drupal/hook_event_dispatcher"]
    }
  },
  "autoload": {
    "psr-4": {
      "Drupal\\settings\\": "src/",
      "Drupal\\settings_branding\\": "modules/settings_branding/src/",
      "Drupal\\settings_communication\\": "modules/settings_communication/src/",
      "Drupal\\settings_social\\": "modules/settings_social/src/"
    }
  }
}
